{
  "pluginName": "墨墨云词本",
  "description": "快速添加单词到墨墨背单词云词本",
  "author": "你的名字",
  "homepage": "https://github.com/your-username/maimemo-utools-plugin",
  "version": "1.0.0",
  "logo": "logo.png",
  "platform": ["win32", "darwin", "linux"],
  "main": "index.html",
  "preload": "preload.js",
  "development": {
    "main": "index.html",
    "preload": "preload.js"
  },
  "features": [
    {
      "code": "maimemo_add_word",
      "explain": "添加单词到墨墨云词本",
      "cmds": [
        "maimemo",
        {
          "type": "regex",
          "label": "添加单词",
          "match": "/^[a-zA-Z\\s]+$/",
          "minLength": 1,
          "maxLength": 100
        }
      ]
    },
    {
      "code": "maimemo_clipboard",
      "explain": "从剪切板添加单词到墨墨云词本",
      "cmds": [
        {
          "type": "over",
          "label": "添加到墨墨",
          "match": "/^[a-zA-Z\\s.,!?;:()\"'-]+$/",
          "minLength": 1,
          "maxLength": 1000
        }
      ]
    }
  ]
}